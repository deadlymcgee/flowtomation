{
  "services": {
    "time of day": {
      "program": "date",
      "parameters": "'+%d/%m/%Y %H:%M:%S'"
    },
    "append to file": {
      "program": ".\/append.py",
      "parameters": "times.txt"
    }
  },
  "flows": {
    "Append time of day": [
      "time of day",
      "append to file"
    ]
  }
}
