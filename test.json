{
  "services": {
    "time of day": {
      "program": "date",
      "parameters": "'+%d/%m/%Y %H:%M:%S'"
    },
    "append to file": {
      "program": ".\/append.py",
      "parameters": "times.txt"
    },
	  "is morning": {
		  "program": ".\/morning.py",
		  "parameters": ""
	  },
	  "is afternoon": {
		  "program": "./afternoon.py",
		  "parameters": ""
	  },
	  "echo": {
		  "program": "echo",
		  "parameters": "input is $$"
	  }
	  
  },
  "flows": {
	  "check time of day": [
		  "time of day",
		  "is morning",
		  "is afternoon"
	  ],
	  "Append time of day": [
      "time of day",
		"echo",
      "append to file"
    ]

	  
  }
}
